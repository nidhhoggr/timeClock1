<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>timeClock</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

{% block stylesheets %}
<link rel="stylesheet" href="{{ asset('bundles/timeclock/css/style.css') }}" type="text/css" media="all" />
<link rel="stylesheet" href="{{ asset('bundles/timeclock/css/jquery.jcarousel.css') }}" type="text/css" media="all" />
<link rel="stylesheet" href="{{ asset('bundles/timeclock/css/jquery-ui.css') }}" type="text/css" media="all" />
<!--[if IE 6]><link rel="stylesheet" href="{{ asset('bundles/timeclock/css/ie6.css') }}" type="text/css" media="all" /><![endif]-->
{% endblock %}
<link rel="shortcut icon" href="{{ asset('bundles/timeclock/images/favicon.ico') }}" />
{% block javascripts %}
<script type="text/javascript" src="{{ asset('bundles/timeclock/js/jquery-1.4.2.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/timeclock/js/jquery.jcarousel.pack.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/timeclock/js/func.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/timeclock/js/jquery-ui.min.js') }}"></script>
</head>

<script type="text/javascript">
    var app_path = "{{asset('bundles/timeclock')}}";
</script>
{% endblock %}

<body>
{% block body %}
<div class="shell">
  <div class="border">
    <div id="header">
      <h1 id="logo">time<em>Clock/em></h1>
      <div class="socials right">
        <ul>
          <li><a href="#" class="rss">RSS</a></li>
          <li><a href="#" class="fb">Facebook</a></li>
          <li class="last"><a href="#" class="twit">Twitter</a></li>
        </ul>
      </div>
      <div class="cl">&nbsp;</div>
    </div>
    <div id="navigation">
      <ul>
        <li>
            <a href="{{ path('ose_time_clock_homepage') }}">Home</a>
        </li>
        <li>
            <a href="{{ path('timeclock/entry') }}">Entries</a>
        </li>
        <li>
            <a href="{{ path('timeclock/entrytype') }}">Entry Types</a>
        </li>
        <li>
            <a href="{{ path('login') }}">Login</a>
        </li>
        <li>
            <a href="{{ path('logout') }}">Logout</a>
        </li>
      </ul>
      <div class="cl">&nbsp;</div>
    </div>
    <div id="main">
      <div id="content" class="left">
      {% block content %}{% endblock %}
      </div>
      <div id="sidebar" class="right">
        <h3>Some Links</h3>
        <div class="sidebar-nav">
          <ul>
            <li>
              <a href="{{ path('ose_time_clock_homepage') }}">Home</a>
            </li>
            <li>
              <a href="{{ path('timeclock/entry') }}">Entries</a>
            </li>
            <li>
              <a href="{{ path('timeclock/entrytype') }}">Entry Types</a>
            </li>
            <li>
              <a href="{{ path('login') }}">Login</a>
            </li>
            <li>
              <a href="{{ path('logout') }}">Logout</a>
            </li>
          </ul>
        </div>
      </div>
      <div class="cl">&nbsp;</div>
    </div>
    <div class="shadow-l"></div>
    <div class="shadow-r"></div>
    <div class="shadow-b"></div>
  </div>
  <div id="footer">
    <p class="left">Copyright &copy; 2013, Supraliminal Solutions, LLC, All Rights Reserved</p>
    <p class="right">Interactive TimeClock for Orlando Software Engineers Meetup</a></p>
    <div class="cl"></div>
  </div>
</div>
</body>
{% endblock %}
</html>
